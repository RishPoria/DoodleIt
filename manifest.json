{
    "manifest_version": 3,
    "name": "Doodle It",
    "description": "Draw something",
    "version": "1.0",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [ "p5.js", "sketch.js"]
        }
    ],
    "action": {
        "default_title": "Doodle It",
        "default_popup": "popup/popup.html"
    }
}